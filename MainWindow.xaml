<Window x:Class="Assignment4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment4"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="300">
    <Grid>
        <StackPanel >
            <Border Height="10"></Border>
            <Label HorizontalAlignment="Center">Enter a Zip Code</Label>
            <Border Height="5"></Border>
            <TextBox Name="uxZipCode" HorizontalAlignment="Center" Width="150" TextChanged="uxZipCode_TextChanged" ></TextBox>
            <Border Height="5"></Border>
            <Button Name="uxSubmit" HorizontalAlignment="Center" Width="80" IsDefault="True" Click="uxSubmit_Click">
                Submit
                <Button.Style>
                    <Style TargetType="Button">
                        <Setter Property="IsEnabled" Value="False" />
                    </Style>
                </Button.Style>
            </Button>
        </StackPanel>
    </Grid>
</Window>
